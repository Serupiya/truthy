{
  "name": "truthy",
  "version": "3.0.0",
  "description": "Create truth tables without the pain",
  "scripts": {
    "lint": "eslint app/scripts/**/*.js test/*.js",
    "test": "karma start --single-run",
    "coverage": "COVERAGE=true npm test && npm run remap-coverage && npm run coverage-report",
    "remap-coverage": "remap-istanbul -i coverage/coverage-unmapped.json -o coverage/coverage-remapped.json -e test/ -e node_modules/",
    "coverage-report": "istanbul report --include coverage/coverage-remapped.json lcov text html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caleb531/truthy.git"
  },
  "keywords": [
    "truth tables",
    "mithril"
  ],
  "author": "Caleb Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caleb531/truthy/issues"
  },
  "homepage": "https://github.com/caleb531/truthy#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "fastclick": "^1.0.6",
    "mithril": "github:MithrilJS/mithril.js#deaf01c8e96446c97f31826b1d04ab4a4e757718",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-brunch": "^6.1.1",
    "brunch": "^2.10.9",
    "chai": "^3.5.0",
    "chai-dom": "^1.6.0",
    "eslint": "^4.18.2",
    "istanbul": "^0.4.5",
    "javascript-brunch": "^2.0.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-static": "^1.0.1",
    "mocha": "^3.2.0",
    "pegjs-brunch": "^1.0.0",
    "postcss-brunch": "github:caleb531/postcss-brunch",
    "remap-istanbul": "^0.9.5",
    "sass-brunch": "github:caleb531/sass-brunch",
    "uglify-js-brunch": "^2.0.1"
  }
}
